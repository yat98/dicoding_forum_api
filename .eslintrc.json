{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["jest"],
    "rules": {
        "semi" : "error",
        "prefer-const" : "error",
        "require-await" : "error",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-sparse-arrays": "off"
    }
}
